                <field name="k2_tags" type="k2tags" extension="k2tags" default="" size="25" label="MOD_HIGHLIGHTER_GK5_K2TAGS" description="MOD_HIGHLIGHTER_GK5_K2TAGS_DESC" class="customHeight" />
